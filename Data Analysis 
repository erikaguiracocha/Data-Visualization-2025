        "import matplotlib.pyplot as plt\n",
        "df_filtered = df[df['Year'].between(2013, 2017)]\n",
        "df['Total Police Division Annual Budget - Hamilton, NJ'] = df['Total Police Division Annual Budget - Hamilton, NJ'].replace({'\\$': '', ',': '', ' ': ''}, regex=True).apply(pd.to_numeric, errors='coerce')\n",
        "df['Total Police Division Annual Budget - Trenton, NJ'] = df['Total Police Division Annual Budget - Trenton, NJ'].replace({'\\$': '', ',': '', ' ': ''}, regex=True).apply(pd.to_numeric, errors='coerce')\n",
        "print(df[['Total Police Division Annual Budget - Hamilton, NJ', 'Total Police Division Annual Budget - Trenton, NJ']].head())\n",
        "plt.figure(figsize=(10, 6))\n",
        "df['Total Police Division Annual Budget - Hamilton, NJ'].plot(kind='hist', bins=10, alpha=0.5, label='Hamilton', color='blue')\n",
